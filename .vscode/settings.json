{
    "editor.formatOnSave": true,
    "Lua.diagnostics.disable": [
        "duplicate-set-field",
        "param-type-mismatch",
        "redundant-parameter",
        "redundant-return-value",
        "undefined-field",
        "undefined-global"
    ],
    "Lua.runtime.builtin": {
        "basic": "disable",
        "debug": "disable",
        "io": "disable",
        "math": "disable",
        "os": "disable",
        "package": "disable",
        "string": "disable",
        "table": "disable",
        "utf8": "disable"
    },
    "Lua.runtime.version": "Lua 5.1",
    "Lua.workspace.ignoreDir": [
        ".vscode",
        "Libs"
    ],
    "Lua.workspace.library": [
        "~/.vscode-server/extensions/ketho.wow-api-0.16.6/Annotations"
    ]
}
